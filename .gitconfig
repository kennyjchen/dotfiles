[core]
	editor = vim
	excludesFile = ~/.gitignore_global
[commit]
	gpgSign = true
[color]
	ui = auto
[alias]
	# Shortening aliases
	a = add
	aa = add --all
	ap = add -p
	b = branch
	ba = branch -a
	bd = branch -d
	c = commit
	cm = commit -m
	co = checkout
	cob = checkout -b
	d = diff
	dc = diff --cached
	f = fetch
	fp = fetch -p
	pl = pull
	ps = push
	rh = reset --hard
	st = status -sb
	sw = switch

	# Feature improving aliases
	acp = !git add --all && git commit -m \"$1\" && git push && :
	rank = shortlog -sn --no-merges
	alias = !git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /

	# Log
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	lga = log --all --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

	# Cleanup aliases
	archivebranch = !git checkout $1 && git switch develop && git tag archive/$1 $1 && git branch -D $1 && git push -d origin $1 && git push --tags && :
	restorebranch = !git checkout -b $1 archive/$1 && :
